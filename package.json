{
	"name": "@salamander.be/vue-gantt",
	"description": "A VueJs Gantt chart.",
	"author": "Frederiek <frederiek@salamander.be>",
	"license": "MIT",
	"version": "0.0.2",
	"main": "./dist/vue-gantt.common.js",
	"keywords": [
		"vue",
		"Gantt"
	],
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"docs": "vue-cli-service build --dest docs",
		"build-bundle": "vue-cli-service build --target lib --name vue-gantt ./src/components/index.js",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"vue": "^2.5.17"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.0.4",
		"@vue/cli-plugin-eslint": "^3.0.4",
		"@vue/cli-service": "^3.0.4",
		"node-sass": "^4.9.0",
		"sass-loader": "^7.0.1",
		"vue-flatpickr-component": "^8.0.0",
		"vue-moment": "^4.0.0",
		"vue-template-compiler": "^2.5.17"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/salamanderbe/vue-gantt"
	},
	"files": [
		"dist/*",
		"src/*",
		"public/*",
		"*.json",
		"*.js"
	],
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
